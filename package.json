  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "compile": "hardhat compile",
    "deploy:testnet": "hardhat run scripts/deploy.ts --network saskeTestnet",
    "deploy:mainnet": "hardhat run scripts/deploy.ts --network saskeMainnet",
    "verify:testnet": "hardhat verify --network saskeTestnet",
    "verify:mainnet": "hardhat verify --network saskeMainnet"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  }
