# Приватный ключ для подписи транзакций
PRIVATE_KEY=your_private_key_here

# RPC URL для сети Saske Mainnet
SASKE_MAINNET_RPC_URL=https://mainnet.saske.xyz

# RPC URL для сети Saske Testnet
SASKE_TESTNET_RPC_URL=https://testnet.saske.xyz

# API ключ для IPFS (если используется)
IPFS_API_KEY=your_ipfs_api_key_here

# API ключ для Etherscan (для верификации контрактов)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Адрес смарт-контракта SaskeEmotionToken (после деплоя)
SASKE_EMOTION_TOKEN_ADDRESS=your_contract_address_here 